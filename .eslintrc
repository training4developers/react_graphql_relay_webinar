{
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"jest": true,
		"commonjs": true
	},
	"extends": [ "eslint:recommended", "plugin:react/recommended" ],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": [ "react" ],
	"rules": {
		"indent": ["error", 4, { "SwitchCase": 1 }],
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"no-console": 0,
		"no-alert": 1,
		"strict": 1
	}
}